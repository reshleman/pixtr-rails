<h1><%= @user.email %>'s Dashboard</h1>

<section>
  <h2>My Galleries</h2>

  <p><%= link_to "New Gallery", new_gallery_path %><p>

  <ul>
    <% @user.galleries.each do |gallery| %>
      <li><%= render gallery %></li>
    <% end %>
  </ul>
</section>

<section>
  <h2>My Groups</h2>

  <p><%= link_to "New Group", new_group_path %></p>

  <ul>
    <% @user.groups.each do |group| %>
      <li><%= render group %></li>
    <% end %>
  </ul>
</section>
